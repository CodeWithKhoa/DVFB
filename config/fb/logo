def dk():
   a= "\033[1;92m═\033[1;94m═"*40
   for i in range(len(a)):
     sys.stdout.write(a[i])
     sys.stdout.flush()
     sleep(0.001)
   print()
def dkngan():
   a= "\033[1;92m═\033[1;94m═"*30
   for i in range(len(a)):
     sys.stdout.write(a[i])
     sys.stdout.flush()
     sleep(0.001)
   print()
